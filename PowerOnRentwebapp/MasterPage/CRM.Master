            <asp:ContentPlaceHolder ID="CPHolder_Form" runat="server">
            </asp:ContentPlaceHolder>
        </div>
        <%--<div class="fixed_page footer" style="height: 32px;">
            <table style="float: right;">
                <tr>
                    <td>
                        <span style="font-size: 12px; color: White;">Powered by</span>
