        </div>--%>
        <!-- end section .footer -->
        <!-- END FOOTER -->
    </div>
    <obout:Flyout runat="server" ID="FlyoutProfileSettings" AttachTo="settings" OpenEvent="ONCLICK"
        CloseEvent="ONMOUSEOUT" Position="BOTTOM_CENTER">
        <div id="flyoutDiv">
            <table cellspacing="5" cellpadding="5" style="text-align: left; margin: 10px;">
                <tr>
                    <td>
                        <a class="linksettings" target="_self" href="../UserManagement/ProfileSetting.aspx">
                            Profile Settings</a>
                    </td>
                </tr>
                <tr>
                    <td>
                        <a class="linksettings" onclick="openChangePassword();">Change Password</a>
                    </td>
                </tr>
                <tr>
                    <td>
                        <a class="linksettings">Change Theme</a>
                    </td>
                </tr>
            </table>
        </div>
    </obout:Flyout>
    <script type="text/javascript">
        function opensettings() {
            <%=FlyoutProfileSettings.getClientID()%>.Open();
        }
        function openChangePassword() {
            window.open("../Login/ChangePassword.aspx", null, 'height=185, width=396,status= 0, resizable= 0, scrollbars=0, toolbar=0,location=0,menubar=0, screenX=0; screenY=0');
        }

        function WaterMark(txt, evt, defaultText) {
            if (txt.value.length == 0 && evt.type == "blur") {
                txt.style.color = "white";
                txt.value = defaultText;
            }
            if (txt.value == defaultText && evt.type == "focus") {
                txt.style.color = "white";
                txt.value = "";
            }
        }
        function clearSession() {
            PageMethods.WMClearSession(null, null);
        }

        function showhideheader() {
            if (top_bar.style.display == 'none') { top_bar.style.display = ""; }
            else if (top_bar.style.display == '') { top_bar.style.display = "none"; }
        }

        function logout(){
            showAlert("You has been Successfully logout !!!","info","../Login/Login.aspx");
            //window.open("../Login/Login.aspx", "_self", 'status= 0, resizable= 0, scrollbars=0, toolbar=0,location=0,menubar=0, screenX=0; screenY=0');
        }

        function CloseShowReport() {
            LoadingOff();
            divPopUp.className = "divDetailExpandPopUpOff";
        }
        function downloadHELP(){
         document.getElementById("iframePORRpt").src = "../POR/Img/CUMMINS MRS User Guide 1.0.0.pdf";
                divPopUp.className = "divDetailExpandPopUpOn";
        }
    </script>
     <style type="text/css">
        .popupClose
        {
            background: url(../App_Themes/Blue/img/icon_close.png) no-repeat;
            height: 32px;
            width: 32px;
            float: right;
            margin-top: -30px;
            margin-right: -25px;
        }
        .popupClose:hover
        {
            cursor: pointer;
        }
        .divDetailExpandPopUpOff
        {
            display: none;
        }
        .divDetailExpandPopUpOn
        {
            border: solid 3px gray;
            width: 65%;
            height: 98%;
            padding: 10px;
            background-color: #FFFFFF;
            margin-top: 50px;
            margin-left: 50px;
            top: 1%;
            left: 3%;
            position: absolute;
            z-index: 99999;
        }
    </style>
    </form>
